% environment paths
path(0,(2,2),0).
path(0,(2,1),1).
path(0,(1,1),2).
path(0,(0,1),3).
path(0,(0,0),4).
path(1,(0,2),0).
path(1,(0,1),4).
path(1,(1,1),5).
path(1,(2,1),6).
path(1,(2,2),7).


% conflict points
conflict(X,Y,T1) :- path(Z1,(X,Y),T1), path(Z2,(X,Y),T2), Z1!=Z2, T1<T2.


